    "fast-animation-frame": "^0.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.1.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "eslint": "^4.17.0",
    "eslint-config-75team": "^1.0.2",
    "http-server": "^0.9.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
